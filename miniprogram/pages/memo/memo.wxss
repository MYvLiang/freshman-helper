/* 头部logo */
.header{
  width: 100%;
  height: 90rpx;
  background-color: #fafafa;
}
.hdCon{
  display: flex;
  height: 100%;
  box-sizing: border-box;
  justify-content: space-between;
  padding: 0 26rpx 10rpx;
  align-items: center;
}
.hdCon navigator{
  height: 50rpx;
}
.hdCon image{
  height: 100%;
  width: 50rpx;
  padding: 0 0;
}
.txt{
  font-size: 50rpx;
  font-weight: 600;
  letter-spacing: 5rpx;
}
.words{
  font-size: 45rpx;
  color: #959499;
  padding: 50% 230rpx;
  letter-spacing: 5rpx;
}
.words text{
  font-size: 30rpx;
  color: #959499;
  letter-spacing: 3rpx;
  padding: 50% 20rpx;
}