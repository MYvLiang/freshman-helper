.jwClub{
  height:70px;
  border-bottom: 1px solid #c7c7c7;
  /* 使其左右排列 */
  display: flex;  
}
.jwClub .jwLogo{
  width:80px;
  height:60px;
  background-color: rgba(241, 232, 232, 0.829);
  margin: 5px;
}
.jwClub .jwLogo image{
  width:100%;
  height:100%;
  border-radius: 50%;
}
.jwClub .jwDiscribe{
  flex:1;
  padding:10px;
  background-color: rgb(226, 241, 247);
}
.jwClub .jwDiscribe text{
  font-size: 12px;
  display: block;
}
.jwClub .jwDiscribe .clubTitle{
  font-size: 14px;
  font-weight: bold;
}
.jwClub .jwDiscribe .discribe{
  color: gray;
  margin:5px 0;
}
