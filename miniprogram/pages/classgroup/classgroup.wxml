 <view class="search">
      <input type="text" class="search-box" placeholder="请输入学院名称" bindinput='getinputvalue' value="{{inputValue}}"></input>
      <icon type="search"></icon>
</view>

<view class="searchresult" wx:if="{{inputValue==''}}">
  <view class="result" wx:for="{{all}}" catchtap="searchbegin" wx:key="index" bindtap="getInputValue" data-postname="{{item.college}}">
    <view>{{item.college}}</view>
  </view>
</view>

<view class="searchresult" wx:if="{{searchresult == true}}">
  <view class="result" wx:for="{{search}}" catchtap="searchbegin" wx:key="index" bindtap="getInputValue" data-postname="{{item.college}}">
    <view>{{item.college}}</view>
  </view>
</view>

<view class="show" wx:if="{{click == true}}" bindtap="getInputValue">
    <view class="title">{{afterclick.college}}</view>
    <view class="qq">群聊:</view>
    <view wx:for="{{afterclick.qun}}" wx:key="index">
      <view>{{item.name}}(<text selectable="true">{{item.qq}}</text>)</view>
      <image src="{{item.pic}}" mode="aspectFit" bindtap="previewImage" bindlongtap="longPressSaveImg" data-src="{{item.pic}}"></image>
    </view>
    <!--<view class="pic" wx:for="{{afterclick.pic}}" wx:key="index"> 
      <image src="{{item}}" mode="aspectFit" bindtap="previewImage" bindlongtap="longPressSaveImg" data-src="{{item}}"></image>
    </view>-->
    <view>助班:</view>
    <view wx:for="{{afterclick.zhuban}}" wx:key="index">
      <view>{{item.class}}</view>
      <view>{{item.name}}(<text selectable="true">{{item.wx}}</text>)</view>
      <image src="{{item.pic}}" mode="aspectFit" bindtap="previewImage" bindlongtap="longPressSaveImg" data-src="{{item.pic}}"></image>
    </view>
    <view class="bottom"><navigator url="../opinion/opinion" class="call">欢迎点击此处为我们提供更多信息</navigator></view>
</view>
