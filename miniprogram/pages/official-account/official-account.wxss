.search{
  height: 90rpx;
}
.search input{
  margin-top: 10rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  height: 70rpx;
  background: white;
  border-radius:5px;
  text-align: center;
}
.list{
  height: 110rpx;
  background: white;
  display: flex;
}
.list image{
  height: 70rpx;
  width: 70rpx;
  border-radius:100%;
  padding: 20rpx 20rpx;
}
.text{
  padding: 30rpx 0 10rpx 0;
}
.divline{
  width: 100%;
  height:2rpx;
  background: white;
}
.line{
  width: 100%;
  height:2rpx;
  background:#f0f3f8;
  margin-left: 100rpx;
}